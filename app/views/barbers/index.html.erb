<%= image_tag('the_team.png', class: 'home_page_pic')%>

<div class="barbers_index">
    <%= content_tag(:h1, 'The Crew') %>

<%if !logged_in? %>
    <% Barber.all.each do |b| %>
        <%= content_tag(:p, b.first_name) %>
    <% end %>
<% else %>
    <% @user_barbers.uniq.each do |b| %>
        <%= content_tag(:p, b.first_name) %>
    <% end %>
<% end %>
</div>