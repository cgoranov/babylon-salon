
<%= form_for(:user, url: login_path) do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email %><br>
    <%= f.label :password %>
    <%= f.password_field :password %><br>
    <%= f.submit "Sign in" %>
<% end %>

    ------- or ---------

<%= button_to 'Sign in with Google', '/auth/google_oauth2' %>

